{
  "compilerOptions": {
      "outDir": "lib",
      "module": "commonjs",
      "target": "es2019",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": false
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
      "node_modules",
      "**/*.spec.ts"
  ]
}